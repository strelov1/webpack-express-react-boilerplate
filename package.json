{
  "name": "webpack-express-react-boilerplate",
  "version": "1.0.0",
  "description": "Webpack-express-react-boilerplate with hot live reload",
  "scripts": {
    "start": "NODE_ENV=prod babel-node server/index.js",
    "watch": "NODE_ENV=dev babel-node server/index.js",
    "build": "NODE_ENV=prod babel-node ./node_modules/webpack/bin/webpack",
    "test": "jest",
    "jest": "jest"
  },
  "author": "Strelov",
  "license": "ISC",
  "dependencies": {
    "express": "^4.15.2",
    "path": "^0.12.7",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "jest": "^22.4.3",
    "react-test-renderer": "^16.3.1",
    "webpack": "^2.3.2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.22.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "env": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}
